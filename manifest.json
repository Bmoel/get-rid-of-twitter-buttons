{
    "manifest_version": 3,
    "name": "Twitter Sidebar Button Hider",
    "version": "1.0.4",
    "description": "Get rid of dumb twitter buttons I don't use",
    "browser_specific_settings": {
      "gecko": {
        "id": "twitter-sidebar-hider@example.com"
      },
      "gecko_android": {
        "id": "twitter-sidebar-hider@example.com"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://x.com/*"],
        "js": ["js/buttonHider.js"],
        "run_at": "document_start"
      }
    ]
  }
  